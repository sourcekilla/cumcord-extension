{
    "name": "Cumcord for Web",
    "description": "A heavy, throbbing, and easy to use cum expeller; now in your browser.",
    "version": "0.0.1",
    "author": "Drake",
    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "content_scripts": [
        {
            "matches": ["*://*.discord.com/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "*://*.discord.com/*"
    ],
    "manifest_version": 2
}
